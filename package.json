{
    "name": "rell-language",
    "displayName": "Rell Language VS Extension",
    "author": {
        "name": "Dennis Kirsch, Duy Chung",
        "email": "mail@dennis-kirsch.de; chungkhanhduy1985@gmail.com"
    },
    "publisher": "DuyChung",
    "description": "Rell Language",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/duymap/rell-vscode.git"
    },
    "contributes": {
        "languages": [{
            "id": "rell",
            "aliases": ["Rell", "rell"],
            "extensions": [".rell"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rell",
            "scopeName": "source.rell",
            "path": "./syntaxes/rell.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "rell",                
                "path": "./snippets/complex-type.json"
            }, 
            {
                "language": "rell",                
                "path": "./snippets/simple-type.json"
            },
            {
                "language": "rell",                
                "path": "./snippets/statements.json"
            }
        ]
    }
}